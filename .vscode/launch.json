{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/server.js",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**",
                "**/lib/**",
                "**/internal/**",
                "**/cmap/**"
            ],
            "smartStep": true,
            "restart": true,
            "console": "integratedTerminal"
        }
    ]
}
